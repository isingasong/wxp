/**app.wxss**/
.container {
  height: 100vh;
  padding: 8px;
  box-sizing: border-box;
  background: #f8f8f8;
  color: rgba(0, 0, 0, 0.85)
} 

.centerd{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.vertical.centerd > view{
  display: flex;
  height: 100%;
  flex-direction: row;
  align-items: center;

}


.header{
  font-size: 32px;
  font-family: serif;

}

.image > image{
  width:100%
}
